tan.\n    setTodos([newTodo, ...todos]);\n  };\n\n  // Ändrar status på en todo.\n  const toggleComplete = (index: number) => {\n    const updatedTodos = todos.map((todo, i) =>\n      i === index ? { ...todo, isCompleted: !todo.isCompleted } : todo\n    );\n    setTodos(updatedTodos);\n  };\n\n  // Tar bort en todo.\n  const deleteTodo = (index: number) => {\n    const updatedTodos = todos.filter((_, i) => i !== index);\n    setTodos(updatedTodos);\n  };\n\n  return (\n    <>\n      <Header />\n\n      <TodoForm onSubmit={addTodo} />\n\n      <ul>\n        {todos.map((todo, index) => (\n          <li\n            key={index}\n            style={{\n              display: \"flex\",\n              alignItems: \"center\",\n              marginBottom: \"8px\",\n            }}\n          >\n            <input\n              type=\"checkbox\"\n              checked={todo.isCompleted}\n              onChange={() => toggleComplete(index)}\n            />\n            <span\n              style={{\n                textDecoration: todo.isCompleted ? \"line-through\" : \"none\",\n                flex: 1,\n              }}\n            >\n              {todo.text}\n            </span>\n            <DeleteButton onDelete={() => deleteTodo(index)} />\n          </li>\n        ))}\n      </ul>\n    </>\n  );\n}\n\nexport default App;\n"],"names":["useState","DeleteButton","Header","TodoForm","App","todos","setTodos","addTodo","text","newTodo","isCompleted","toggleComplete","index","updatedTodos","map","todo","i","deleteTodo","filter","_","onSubmit","ul","li","style","display","alignItems","marginBottom","input","type","checked","onChange","span","textDecoration","flex","onDelete"],"mappings":";AAAA,SAASA,QAAQ,QAAQ,QAAQ;AACjC,OAAO,YAAY;AACnB,OAAOC,kBAAkB,4BAA4B;AACrD,OAAOC,YAAY,sBAAsB;AACzC,OAAOC,cAAc,wBAAwB;AAO7C,SAASC;IACP,MAAM,CAACC,OAAOC,SAAS,GAAGN,SAAiB,EAAE;IAE7C,wCAAwC;IACxC,MAAMO,UAAU,CAACC;QACf,MAAMC,UAAU;YAAED;YAAME,aAAa;QAAM;QAC3C,kDAAkD;QAClDJ,SAAS;YAACG;eAAYJ;SAAM;IAC9B